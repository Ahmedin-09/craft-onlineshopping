#%RAML 1.0 SecurityScheme
type: OAuth 2.0
description: "This api support OAuth 2.0"
describedBy:
 headers:
   Authorization:
     description: "token is required for this transaction"
   queryparameters:
   access_token:
     description: "users have to send valid token with the quaryparameter"
     type: string
 responses:
       401:
        body:
          application/json:
            example:
              {
               message: "bad request"
              }
       403:
         description: "not allow to access the resource"


  
